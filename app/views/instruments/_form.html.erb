<% if flash[:errors] %>
    <ul>
        <% flash[:errors].each do |error| %>
            <li><%= error %></li>
        <% end %>
    </ul>
<% end %>

<%= form_for @instrument do |f| %>

    <%= f.label :name %>
    <%= f.text_field :name %><br>

    <%= f.label :classification %>
    <%= f.text_field :classification %><br>

    <%= f.submit %>
  
<% end %>